{
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@5.5.16"
    }
  },
  "rewrites": [
    {
      "source": "/api/polymarket/(.*)",
      "destination": "/api/polymarket/[...path]"
    },
    { "source": "/api/event-state", "destination": "/api/data/event-state" },
    { "source": "/api/event-state/(.*)", "destination": "/api/data/event-state" },
    { "source": "/api/strategy-config", "destination": "/api/data/strategy-config" },
    { "source": "/api/strategy-config/(.*)", "destination": "/api/data/strategy-config" },
    { "source": "/api/trades", "destination": "/api/data/trades" },
    { "source": "/api/trades/(.*)", "destination": "/api/data/trades" },
    { "source": "/api/positions", "destination": "/api/data/positions" },
    { "source": "/api/positions/(.*)", "destination": "/api/data/positions" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
